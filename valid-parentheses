class Solution {
public:
    bool isValid(string s) {
        if (s.empty()) return true;
        string st;
        map<char, char> mp;
        mp['('] = ')';
        mp['{'] = '}';
        mp['['] = ']';
        st.push_back(s[0]);
        for (int i = 1; i < s.size(); i++) {
            if (!st.empty() && mp[st.back()] == s[i])
                st.pop_back();
            else
                st.push_back(s[i]);
        }
        return st.empty();
    }
};
