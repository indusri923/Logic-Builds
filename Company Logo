# Company Logo problem solution

if __name__ == '__main__':
    s = input().strip()
    freq = {}
    # Count frequencies
    for ch in s:
        freq[ch] = freq.get(ch, 0) + 1
    # Sort by (-count, character)
    sorted_freq = sorted(freq.items(), key=lambda x: (-x[1], x[0]))
    # Print top 3
    for char, count in sorted_freq[:3]:
        print(char, count)
